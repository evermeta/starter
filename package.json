{
  "name": "starter",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/main.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --verbose",
    "test:coverage:watch": "jest --coverage --verbose --watch",
    "start": "ts-node src/main.ts",
    "docs": "typedoc --options typedoc.config.ts",
    "docs:watch": "typedoc --options typedoc.config.ts --watch",
    "docs:generate": "compodoc -p tsconfig.json -s",
    "docs:serve": "compodoc -p tsconfig.json -s -w",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Production-ready TypeScript starter project",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.54.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.56.0",
    "@opentelemetry/sdk-node": "^0.56.0",
    "@types/node": "^20.0.0",
    "dotenv": "^16.4.7",
    "inversify": "^6.0.1",
    "prom-client": "^14.0.1",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@compodoc/compodoc": "^1.1.26",
    "@opentelemetry/sdk-trace-base": "^1.29.0",
    "@testcontainers/postgresql": "^10.16.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "express": "^4.21.2",
    "husky": "^9.1.7",
    "jest": "^29.0.0",
    "pg": "^8.13.1",
    "supertest": "^7.0.0",
    "swagger-ui-express": "^5.0.1",
    "testcontainers": "^10.16.0",
    "ts-jest": "^29.2.5",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.27.3",
    "typedoc-plugin-markdown": "^3.17.1"
  }
}
